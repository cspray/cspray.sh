{
  "name": "cspray/cspray.sh",
  "require": {
    "php": "^8.1",
    "cspray/annotated-container": "^0.4",
    "nunomaduro/termwind": "^1.10",
    "symfony/console": "^6.0",
    "php-di/php-di": "v7.x-dev",
    "symfony/process": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Cspray\\SprayShell\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cspray\\SprayShell\\": "tests"
    }
  },
  "require-dev": {
    "pestphp/pest": "^1.21",
    "mockery/mockery": "^1.5",
    "symfony/test-pack": "^1.0",
    "mikey179/vfsstream": "^1.6"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
